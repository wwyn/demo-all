/* pages/generate/generate.wxss */

.generate {
  width: 100%;
  height: 1201rpx;
  background-color: #f6f8fb;
  padding: 40rpx 30rpx;
  box-sizing: border-box;
}
.generate view {
   font-size: 28rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  margin-bottom: 20rpx;
}


.generate .address {
  width: 690rpx;
  height: 91rpx;
  background: rgba(235, 238, 242, 1);
  border-radius: 8px;
  display: block;
  margin: 0 auto;
  padding: 0 10rpx;
  box-sizing: border-box;
  font-size: 28rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(74, 74, 74, 1);
  line-height: 91rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  padding-right: 112rpx;
}

.copy {
  width: 112rpx;
  height: 91rpx;
  background: rgba(52, 140, 248, 1);
  border-radius: 0px 8px 8px 0px;
  color: #fff;
  position: absolute;
  right: 30rpx;
  top: 96rpx;
  z-index: 2;
  text-align: center;
  line-height: 91rpx;
  font-size: 28rpx;
}
.generate .expert {
  width: 670rpx;
  height: 98rpx;
  background-color: #7bc8fd;
  font-size: 36rpx;
  color: #fff;
  text-align: center;
  line-height: 98rpx;
  margin: 0 auto;
  border-radius: 12rpx;
  margin-top: 210rpx;
}

.generate .enterprise {
  width: 670rpx;
  height: 98rpx;
  background-color: #2865fb;
  font-size: 36rpx;
  color: #fff;
  text-align: center;
  line-height: 98rpx;
  margin: 0 auto;
  margin-top: 30rpx;
  margin-bottom: 26rpx;
  border-radius: 12rpx;
}
